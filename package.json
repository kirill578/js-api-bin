{
  "name": "js-api-bin-server",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev-heroku": "REDISCLOUD_URL=$(heroku config:get REDISCLOUD_URL -a js-api-bin) npm run dev",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "redis": "^2.8.0",
    "socket.io": "^2.1.1",
    "uuid": "^3.3.2",
    "vm2": "^3.6.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "engines": {
    "node": "10.12.0"
  }
}
